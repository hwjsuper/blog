<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="u/<%= post.name %>/<%= post.time%>/<%= post.title %>"><%= post.title %></a></h2></p>
  <p class="info">
    作者：<a href="u/<%= post.name %>"><%= post.name %></a> |
    日期：<%= post.time %>
    标签：
      <% for(var i in post.tags) { %>
        <% if (post.tags[i]) { %>
          <a class="tag" href="/tags/<%= post.tags[i] %>"><%= post.tags[i] %></a>
        <% } %>
      <% } %>
  </p>
  <p><%- post.post %></p>
  <% }) %>
<%- include footer %>